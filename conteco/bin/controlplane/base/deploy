#!/usr/bin/env bash
if [[ "$1" == "for-each-node" ]] ; then
(
	shift
  . executionplane-invoke "$0 $@"
	executionplane-error "Method not implemented: ${CONTECO_NAME} $(basename $0)."
	executionplane-complete
)
else
    export PATH="$CONTECO_EXECUTIONPLANE_APIINTERNALPATH_BASE"
	for-each $(basename $0) $@
fi
