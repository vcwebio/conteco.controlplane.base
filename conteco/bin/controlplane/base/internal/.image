#!/usr/bin/env bash
(
    . executionplane-invoke "$0 $@"
    case "$1" in
		build)
			executionplane docker image $@
			;;
		prune)
			executionplane docker image $@
			;;
		pull)
			executionplane docker image $@
			;;
		push)
			executionplane docker image $@
			;;
		rm)
			executionplane docker image $@
			;;
		tag)
			executionplane docker image $@
			;;
        *)
            executionplane-error "Method not recognised: $1."
            ;;
    esac
    executionplane-wait-for-output-parser
    executionplane-complete
)
