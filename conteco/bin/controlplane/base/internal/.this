#!/usr/bin/env bash
(
    . executionplane-invoke "$0 $@"
	executionplane-message "" ""
	.container run -v $CONTECO_PWD_VOLUME:/conteco/pwd "$CONTECO_REALM/$CONTECO_ECOSYSTEM.$CONTECO_TYPE.$CONTECO_NAME" \
		--executiontag ${CONTECO_EXECUTIONTAG} --interactive .this $@ --transparent
	executionplane-message "" ""
	executionplane-complete
)
