#!/usr/bin/env bash
imageName=$1
action=$2
shift
shift

. executionplane-capture-output docker create $imageName
tmpfile=$(mktemp /tmp/scriptfile.XXXXXX)
executionplane docker container cp $CONTECO_EXECUTIONPLANE_OUTPUT:/conteco/extract/controlplane/$action $tmpfile
chmod 777 $tmpfile
$tmpfile $@
rm $tmpfile
