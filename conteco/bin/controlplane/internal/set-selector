#!/usr/bin/env bash

selector="$1"
if [[ "$selector" == "." ]] ; then
	selector=
fi
if [[ $selector != "${CONTECO_REALM}/${CONTECO_ECOSYSTEM}."* && $selector != "${CONTECO_REALM}/${CONTECO_ECOSYSTEM}" ]] ; then
	if [[ $selector == "${CONTECO_ECOSYSTEM}."* ]] ; then
		selector="${CONTECO_REALM}/${CONTECO_ECOSYSTEM}.${selector}"
	else
		selector="${CONTECO_REALM}/${CONTECO_ECOSYSTEM}.${selector}"
	fi
fi
echo $selector
