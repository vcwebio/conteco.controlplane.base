echo "Starting controlplane.${CONTECO_NAME}"
mode=$1
if [[ "$mode" == "" && "$mode" != "modeco" ]] ; then
	mode="conteco"
fi
${CONTECO_SUDO}docker run -it -v /var/run/docker.sock:/var/run/docker.sock -v $(pwd):/conteco/pwd ${CONTECO_REALM}/${CONTECO_ECOSYSTEM}.controlplane.$mode --interactive start $mode linux
